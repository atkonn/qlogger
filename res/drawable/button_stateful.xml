<?xml version="1.0" encoding="utf-8" ?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
  <item 
    android:state_pressed="true">
    <shape>
      <solid 
        android:color="#99eca14f" />
      <corners
        android:radius="10dip" />
    </shape>
  </item>
  <item
    android:state_focused="true" >
    <shape>
      <solid
        android:color="#66eca14f" />
      <corners
        android:radius="10dip" />
    </shape>
  </item>
  <item>
    <shape>
      <solid
        android:color="#00eca14f" />
      <corners
        android:radius="10dip" />
    </shape>
  </item>
</selector>
